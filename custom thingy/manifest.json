{
  "manifest_version": 3,
  "name": "Notey",
  "version": "1.0",
  "description": "A simple note-taking extension",
  "permissions": [
   "storage",
   "activeTab",
   "scripting"
  ],
  "host_permissions": [
   "https://www.googleapis.com/*"
  ],
  "background": {
   "service_worker": "background.js"
  },
  "action": {
   "default_popup": "popup.html"
  }
 }
 